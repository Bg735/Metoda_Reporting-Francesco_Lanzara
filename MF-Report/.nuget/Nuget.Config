<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear/>
		<!-- ensure only the sources defined below are used -->
		<add key="MF-SR2" value="https://srv-tfs02.metoda.it/tfs/Finance/SmartregEE2_0/_packaging/MF-SR2/nuget/v3/index.json"/>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json"/>
	</packageSources>
	<!-- Define mappings by adding package patterns beneath the target source. -->
	<!-- Contoso.* packages and NuGet. Common will be restored from contoso.com, everything else from nuget.org. -->
	<packageSourceMapping>
		<!-- key value for <packageSource> should match key values from <packageSources> element -->
		<packageSource key="nuget.org">
			<package pattern="*"/>
		</packageSource>
		<packageSource key="MF-SR2">
			<package pattern="Evo.*"/>
			<package pattern="IdentityClient.*"/>
			<package pattern="MF-*"/>
			<package pattern="MF.*"/>
			<package pattern="Session.*"/>
			<package pattern="SmartWork.*"/>
			<package pattern="Drive.*"/>
		</packageSource>
		<packageSource key="MF-SR2-Local">
			<package pattern="Evo.*"/>
			<package pattern="IdentityClient.*"/>
			<package pattern="MF-*"/>
			<package pattern="MF.*"/>
			<package pattern="Session.*"/>
			<package pattern="SmartWork.*"/>
			<package pattern="Drive.*"/>
		</packageSource>
	</packageSourceMapping>
</configuration>