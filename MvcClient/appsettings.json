{
  "Logging": {
    "LogLevel": {
        "Default": "Debug",
        "Microsoft.AspNetCore": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Authority": "http://gateway:8080",
    "ClientId": "mvcclient",
    "ClientSecret": "secret_mvcclient"
  },
  "Storage": {
    "Root": "/app/docstorage"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/appdata/docmetadata.db;Cache=Shared;Pooling=False"
  },
  "ApiProvider": {
    "BaseUrl": "http://restapi:8080/api/"
  }
}
